{
  "name": "lullaibot-education-app",
  "version": "0.1.0",
  "private": true,
  "description": "An application that enhances the functionality of the Lullabot AI Education Wiki",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.6",
    "@mui/material": "^6.4.6",
    "@types/uuid": "^10.0.0",
    "axios": "^1.8.1",
    "bcryptjs": "^3.0.2",
    "capture-website": "^4.2.0",
    "chart.js": "^4.4.8",
    "cookie-parser": "^1.4.7",
    "date-fns": "^2.30.0",
    "formik": "^2.4.3",
    "framer-motion": "^10.16.4",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.476.0",
    "marked": "^9.0.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.16.0",
    "uuid": "^11.1.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitejs/plugin-react": "^4.0.4",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.19",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.3"
  },
  "scripts": {
    "dev:frontend": "vite",
    "dev:backend": "node src/server/index.js",
    "dev": "concurrently \"npm run mongo:check && npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "mongo:check": "node src/server/utils/mongoManager.js check",
    "mongo:start": "node src/server/utils/mongoManager.js start",
    "prestart": "npm run mongo:check"
  }
}
